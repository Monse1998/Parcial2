from turtle import *
# Creacion del laberinto (kuro)
kuro=Turtle()
kuro.penup()
kuro.color("black")
kuro.pensize(10)
kuro.goto(-150,190)
kuro.pendown()
kuro.forward(100)
kuro.right(90)
kuro.forward(80)
kuro.backward(80)
kuro.left(90)
kuro.forward(400)
kuro.right(90)
kuro.forward(170)
kuro.right(90)
kuro.forward(80)
kuro.right(90)
kuro.forward(80)
kuro.backward(80)
kuro.left(90)
kuro.forward(100)
kuro.left(90)
kuro.forward(80)
kuro.right(90)
kuro.forward(200)
kuro.left(90)
kuro.forward(80)
